
{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-console": "error",
      "no-eval": "error",
      "quotes": [2, "single", "avoid-escape"],
      "no-empty-function": "error",
      "no-unused-vars": 0
    }
  }